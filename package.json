{
	"name": "boiler-plate-front",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "cross-env NODE_ENV=development vite --host --mode dev",
		"dev:release": "cross-env NODE_ENV=production vite --host --mode release",
		"build": "cross-env NODE_ENV=production tsc && vite build --mode dev",
		"build:dev": "cross-env NODE_ENV=development tsc && vite build --mode dev",
		"build:release": "tsc && vite build --mode release NODE_ENV=production",
		"preview": "vite preview",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
		"format": "npm run prettier --write --fix ./src && npm run lint",
		"prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
		"assets": "node src/scripts/assetProcess.cjs public/images",
		"prepare": "husky"
	},
	"dependencies": {
		"@tanstack/react-query": "^5.84.2",
		"axios": "^1.11.0",
		"classnames": "^2.5.1",
		"dayjs": "^1.11.13",
		"qs": "^6.14.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-router-dom": "^7.8.0",
		"sass": "^1.90.0",
		"scss-reset": "^1.4.6",
		"zustand": "^5.0.7"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.33.0",
		"@types/node": "^24.2.1",
		"@types/qs": "^6.14.0",
		"@types/react": "^19.1.9",
		"@types/react-dom": "^19.1.7",
		"@typescript-eslint/eslint-plugin": "^8.39.0",
		"@typescript-eslint/parser": "^8.39.0",
		"@vitejs/plugin-react": "^5.0.0",
		"cross-env": "^10.0.0",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.5",
		"prettier": "^3.6.2",
		"rollup-plugin-visualizer": "^6.0.3",
		"typescript": "^5.9.2",
		"vite": "^7.1.1",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.2",
		"vite-plugin-svgr": "^4.3.0"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"prettier --write",
			"eslint --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix"
		],
		"*.{json,md,scss,css}": [
			"prettier --write"
		]
	}
}
